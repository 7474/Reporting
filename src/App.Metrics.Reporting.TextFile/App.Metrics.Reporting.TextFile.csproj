<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\build\common.props" />
    
  <PropertyGroup>
    <Description>Provides text file reporting capability to App Metrics</Description>
    <AssemblyTitle>App.Metrics.Reporting.TextFile</AssemblyTitle>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <PackageTags>appmetrics;reporting;influxdb</PackageTags>    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="App.Metrics.Formatters.Ascii" Version="$(AppMetricsCoreVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\App.Metrics.Reporting\App.Metrics.Reporting.csproj" />
  </ItemGroup>

</Project>
