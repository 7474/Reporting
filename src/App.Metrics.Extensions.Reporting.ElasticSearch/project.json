{
  "authors": [ "Allan Hardy", "Jenya Y." ],

  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "debugType": "full",
    "additionalArguments": [ "/ruleset:../../AppMetrics.ruleset", "/additionalfile:../../stylecop.json" ]
  },
  "copyright": "Allan Hardy 2016",

  "dependencies": {
    "App.Metrics": "1.0.0-rc1",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "description": "Provides ElasticSearch reporting capability to App Metrics",

  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    },
    "net452": {
      "buildOptions": {
        "define": [
          "CLASSIC"
        ]
      },
      "dependencies": {

      },
      "frameworkAssemblies": {
      }

    }
  },

  "packOptions": {
    "tags": [ "Metrics", "Monitoring", "Instrumentation", "Reporting", "ElasticSearch" ],
    "projectUrl": "https://alhardy.github.io/app-metrics-docs/getting-started/reporting/index.html#es-reporter",
    "licenseUrl": "https://github.com/alhardy/AppMetrics.Reporting/blob/master/LICENSE",
    "iconUrl": "https://alhardy.github.io/app-metrics-docs/logo.png",
    "repository": {
      "type": "git",
      "url": "git://github.com/alhardy/AppMetrics.Reporting"
    }
  },
  "title": "App.Metrics.Extensions.Reporting.ElasticSearch",
  "version": "1.0.0-rc1"
}
